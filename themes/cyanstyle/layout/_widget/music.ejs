<% if (theme.music){ %>
  <aside class="widget">
    <h3 class="widget-title">Music</h3>
    <div class="widget-content">
    	<%= theme.music.name %>
      <video autoplay="autoplay" name="media" loop="loop">
      	<source src="<%= theme.music.url %>" type="audio/mpeg">
      </video>
    </div>
  </aside>
<% } %>